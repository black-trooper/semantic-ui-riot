{
  "name": "semantic-ui-riot",
  "version": "1.2.0",
  "description": "Semantic UI for Riot",
  "main": "dist/semantic-ui-riot.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev-watch & npm run compile & npm run test-watch",
    "dev": "webpack --mode development",
    "dev-watch": "webpack --mode development --watch --hot",
    "prod": "webpack --mode production",
    "clean": "rimraf ./coverage && rimraf ./dist/tags",
    "compile": "npm run clean && riot -w tags dist/tags",
    "test": "npm run clean && riot tags dist/tags && karma start",
    "test-watch": "karma start --no-single-run",
    "build": "npm run test && npm run dev && npm run prod"
  },
  "keywords": [
    "riotjs",
    "riot",
    "semantic",
    "semanticui"
  ],
  "author": {
    "name": "black-trooper",
    "url": "https://github.com/black-trooper/semantic-ui-riot/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/black-trooper/semantic-ui-riot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/black-trooper/semantic-ui-riot/issues"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "q": "^1.5.1",
    "riot": "^3.13.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015-riot": "^1.1.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^3.1.1",
    "htmlhint-loader": "^1.3.1",
    "htmlhint-riot-rules": "^0.3.14",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jquery": "^3.3.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-coveralls": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-riot": "^2.0.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^4.0.1",
    "riotjs-loader": "^4.0.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.13.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.3.0",
    "webpack-date-fns-externals": "^1.0.2"
  }
}
